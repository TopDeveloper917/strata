    "style": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@bonfida/spl-name-service": "^0.1.22",
    "@project-serum/common": "^0.0.1-beta.3",
    "@solana/web3.js": "^1.29.2",
    "@strata-foundation/spl-token-account-split": "^0.2.7",
    "@strata-foundation/spl-token-bonding": "^0.2.7",
    "@strata-foundation/spl-token-staking": "^0.2.7",
    "@strata-foundation/spl-utils": "^0.2.7",
    "bn.js": "^5.2.0",
    "copyfiles": "^2.4.1"
  },
  "devDependencies": {
    "prettier": "2.3.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4"
  },
  "gitHead": "4437fa25d76d796d0ea410097b8f98e097397a0b"
